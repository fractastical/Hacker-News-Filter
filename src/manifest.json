{
  "name": "HN Filter",
  "version": "1.0",
  "description": "Hides stories below a certain karma rating and filters for keywords.",
  "content_scripts": [{
    "matches": ["http://news.ycombinator.com/"],
    "js": ["filter.js"]
  }],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs", "http://news.ycombinator.com/"
  ]
}